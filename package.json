{
  "name": "wfp-ui-reactjs",
  "version": "0.0.15",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "WFP RMTD Team",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "buildb": "babel src -d lib",
    "build": "webpack",
    "watch": "webpack --config webpack.config.js -w",
    "lint": "eslint src/**; exit 0",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/jestTestsSetup.jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.5.2",
    "browser-sync": "^2.2.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "dateformat": "^3.0.2",
    "del": "^2.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "es6-promise": "^4.1.1",
    "eslint": "^4.7.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.2.1",
    "main-bower-files": "^2.5.0",
    "mathsass": "^0.10.1",
    "node-sass": "^4.5.3",
    "polished": "^1.4.1",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react-paginate": "^4.4.4",
    "react-select": "^1.0.0-rc.10",
    "react-test-renderer": "^15.6.2",
    "react-tippy": "^1.2.2",
    "run-sequence": "^1.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "styled-components": "^2.1.2",
    "webpack": "^3.6.0",
    "wiredep": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  }
}
